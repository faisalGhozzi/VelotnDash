<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="747.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.velotn.ui.front.products.ProductsController">
   <children>
      <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="1000.0" spacing="100.0">
         <children>
            <Button fx:id="frontAllProductBtn" mnemonicParsing="false" onAction="#loadAllProducts" stylesheets="@../../../css/style.css" text="All">
               <HBox.margin>
                  <Insets left="50.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="frontBikeProductBtn" mnemonicParsing="false" onAction="#loadBikesProducts" stylesheets="@../../../css/style.css" text="Bikes">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Button>
            <Button fx:id="frontAccProductBtn" mnemonicParsing="false" onAction="#loadAccessoriesProducts" stylesheets="@../../../css/style.css" text="Accessories">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="frontPartsProductBtn" mnemonicParsing="false" onAction="#loadPartsProducts" stylesheets="@../../../css/style.css" text="Parts" />
            <Button fx:id="frontRentProductBtn" mnemonicParsing="false" onAction="#loadRentProducts" stylesheets="@../../../css/style.css" text="Rent">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
         </children>
      </HBox>
      <AnchorPane fx:id="productDisplayArea" layoutY="100.0" prefHeight="647.0" prefWidth="1000.0" />
   </children>
</AnchorPane>
